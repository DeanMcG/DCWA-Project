
<button id="myButton">Home</button>
<table border="3">

    <tr>
        <th>Country Code</th>
        <th>Country Name</th>
        <th>Country Details</th>
    </tr>

    <% countries.forEach((country) => { %>
        <tr>
            <td><%= country.co_code %></td>
            <td><%= country.co_name %></td>
            <td><%= country.co_details %></td>
        </tr>
        <% }) %>

</table>
<button id="addCountry">Add Country</button>

<script>
var button = document.getElementById('myButton');
button.onclick = function() {
  location.assign('/');
}

var button = document.getElementById('addCountry');
button.onclick = function() {
  location.assign('/addCountry');
}
</script>
